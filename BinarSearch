let arr=[17,6,11,9,13,17] 
for(let i=0;i<arr.length;i++){ 
    for(let j=0;j<arr.length-1;j++){ 
        if(arr[j]>arr[j+1]){ 
            let p=arr[j] 
            arr[j]=arr[j+1] 
            arr[j+1]=p 
        } 
    } 
} 
