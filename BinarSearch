let Mas=[{name:'Ira',age:'15'},
{name:'Filip',age:'13'},
{name:'Leo',age:'17'}] 
for(let i=0;i<Mas.length-1;i++){ 
    for(let j=0;j<Mas.length-1;j++){ 
        if(Mas[j]>Mas[j+1]){ 
            let p=Mas[j] 
            Mas[j]=Mas[j+1] 
            Mas[j+1]=p 
        } 
    } 
} 
