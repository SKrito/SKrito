let mas=[{name:'Filip',age:'13'},
{name:'Ira',age:'15'},
{name:'Leo',age:'17'}] 
function Searhc(mas,e1){ 
    let idx=-1; 
    for(let i='0';i<mas.length;i++){ 
        if(mas[i]===e1){ 
            idx=i; 
            break; 
        } 
    } 
    return idx 
} 
